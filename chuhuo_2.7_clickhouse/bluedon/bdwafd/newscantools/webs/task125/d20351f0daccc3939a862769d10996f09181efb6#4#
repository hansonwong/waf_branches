				itemsTag:'.scrollItems',
				itemSize:document.documentElement.clientWidth<1235?185:195,
				originalIndex:0,
				speed:300,
				easing:null,
				beforeStart:function(){},
				afterEnd:function(current,__this){
					var displied = __this.__data.displied;
					var tabLeft = $("#index_show_shortvideo_tv .tabLeft");
					var tabRight = $("#index_show_shortvideo_tv .tabRight");

						if( $.inArray(0,displied) !== -1 ){
							tabRight.show();
							tabLeft.hide();
						}else if( $.inArray( __this.__data.allItems.length-1 ,displied)!==-1 ){
							tabRight.hide();
							tabLeft.show();
