    scriptCharset: 'utf-8',
    success: function (data) {
        // data[0]['data']['videos'][0]['id'] = '0821-10:00$$0829-12:00';
        // data[0]['data']['videos'][0]['update'] = '110';
        if (data && data[0]['data']['videos'].length > 1) {
            var getcookieid = data[0]['data']['videos'][0]['update'];
            var cookieid = $.cookie.get(self.cookie);
            if (getcookieid !== cookieid && parseInt(getcookieid, 10) !== 0) {
                require("common:widget/video/push/push.js")(data[0]['data']['videos']);
