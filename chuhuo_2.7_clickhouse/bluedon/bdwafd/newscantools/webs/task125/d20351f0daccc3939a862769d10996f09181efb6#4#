                            $.log("http://nsclick.baidu.com/u.gif?pid=104&bl=index_right_qipao&ext=open");
                        }

                        $("#index_right_shixiao").html(tpl({data: adItem}));

                        $(".pop-wrap-big .pop-close").off("click").on("click", function(e){
                            e.preventDefault();
