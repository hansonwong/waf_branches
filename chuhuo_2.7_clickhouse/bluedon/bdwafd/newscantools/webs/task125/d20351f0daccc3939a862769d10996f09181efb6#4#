				afterEnd:function(a,b){
                    var current ;
                    var Ckey = [3,0,1,2,3,0]; 
                    current = b.current > 1 ? Ckey[Math.floor(b.current%num)] : Ckey[4-Math.abs(Math.floor(b.current%num))];

					$(".carousel-dotted-list .dotted-item").removeClass("dotted-selected");
