		visible = num < 3 ? 1 : 3;
	var c = new carousel({
				itemsTag:".carousel-item",
				itemSize:document.documentElement.clientWidth<1235?1000:1235,
				originalIndex:0,
				duration:7000,
				speed:500,
				easing:null,//"easeInOutCubic",
