								afterEnd:function(a,b){
									$(".bdv-carousel-dot li").removeClass("dot-selected");
									$(".bdv-carousel-dot li").eq(Math.floor(a%9)).addClass("dot-selected");
								},
								visible:1,
								step:1,
								auto:true

							});
					

