!function(){    ;(function(){
        var $ = require("common:static/vendor/jquery/jquery.js");

		$(function() {
            var bdPassPop = require('common:static/ui/bdPassPop/bdPassPop.js'); 
            var $body = $("body");
            $body.on("mouseenter","#todayHotMeiNv .item", function() {
                if(!$body.hasClass('global-logged')) {
                    var $meinvLogin = $(this).find(".meinv-login");
                    if(!$(this).hasClass("item-xiuba")) {
                        $meinvLogin.show();
                    }
                }
            });
            $body.on("mouseleave", "#todayHotMeiNv .item", function() {
                var $meinvLogin = $(this).find(".meinv-login");
                $meinvLogin.hide();
            });
            $("body").on("click", "#todayHotMeiNv .item", function(e) {
                if(!$body.hasClass('global-logged')) {
                    if(!$(this).hasClass("item-xiuba")) {
                        e.preventDefault();
                        bdPassPop.show();
                    }
                }
            });
            // 新增美女区块
            $body.on("mouseenter","#showBeauty .item", function() {
                if(!$body.hasClass('global-logged')) {
                    var $meinvLogin = $(this).find(".meinv-login");
                    $meinvLogin.show();
                }
            });
            $body.on("mouseleave", "#showBeauty .item", function() {
                var $meinvLogin = $(this).find(".meinv-login");
                $meinvLogin.hide();
            });
            // 带图区：点击调起登录
            $("body").on("click", "#showBeauty .item", function(e) {
                if(!$body.hasClass('global-logged')) {
                    e.preventDefault();
                    bdPassPop.show();
                }
            });
            // 排行榜：点击调起登录
            $("body").on("click", "#topBeauty li a", function(e) {
                if(!$body.hasClass('global-logged')) {
                    e.preventDefault();
                    bdPassPop.show();
                }
            });
		})    
    })();
	}();
