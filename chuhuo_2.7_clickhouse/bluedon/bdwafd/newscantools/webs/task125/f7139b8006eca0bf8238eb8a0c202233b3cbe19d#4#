!function(){require.async(["common:static/ui/carousel/jCarouselLite.js", "common:static/vendor/jquery/jquery.js"],function(carousel,$){
	var itemSize = document.documentElement.clientWidth<1235?1000:1235;
	var c = new carousel({
				itemsTag:".carousel-item",
				itemSize:itemSize,
				originalIndex:0,
				duration:5000,
				speed:300,
				easing:null,
				beforeStart:function(){},
				afterEnd:function(a,b){
					$("#focus .music-carousel-control ul li").removeClass("item-selected");
					$("#focus .music-carousel-control ul li").eq(Math.floor(a%6)).addClass("item-selected");
				},
				auto:true,
				visible:1,
				step:1
			});
	
	c.render("#focus .carousel-container");	
	$("#focus  .music-carousel-control ul li").each(function(index,ele){
		$(ele).on("mousemove",function(){
			if(!$(this).hasClass("item-selected")){
				c.go(index);
			}
		})
	});

	$("#focus .leftCtrl").on("click",function(){
		c.prev();
	});;

	$("#focus .rightCtrl").on("click",function(){
		c.next();
	});;

	$(window).on("window:resize",function(arg,key){
		if(key.type =="big" || key.type =="w1280"){
			c.refresh(1235)	
		}
		if(key.type=="small"){
			c.refresh(1000);	
		}
	});
});
}();
!function(){
	var $content = $("#content");
	
	var showIntro = function(e){
		var $target = $(e.target);
		var $section = $target.parents('.section');
		var $bigImgIntro = $section.children('.bigImgIntro');
		$bigImgIntro.css("display","block");
		$bigImgIntro.stop().animate({
			opacity: "0.95"
		},1000);
	}


	var hideIntro = function(e){
		var $target = $(e.target);
		var $section = $target.parents('.section');
		var $bigImgIntro = $section.children('.bigImgIntro');
		$bigImgIntro.stop().animate({
			opacity: "0"
			},1000,function(){
			$bigImgIntro.css("display","none");
		});
	}

	$content.delegate(".leftBigImg","mouseover",showIntro);
	$content.delegate(".leftBigImg","mouseout",hideIntro);
	
}();
