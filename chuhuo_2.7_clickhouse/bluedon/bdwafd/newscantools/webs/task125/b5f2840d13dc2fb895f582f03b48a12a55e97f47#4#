                if (!ret || !ret.length || !ret[0]) return;
                var item = ret[0].data.videos;
                _.each(item, function(adItem){
                    if (adItem.sub_title == "index_right_qipao"){
                        if ($.cookie.getRaw("_bdvqipao") == -1){
                            adItem.hot_week = 1;
