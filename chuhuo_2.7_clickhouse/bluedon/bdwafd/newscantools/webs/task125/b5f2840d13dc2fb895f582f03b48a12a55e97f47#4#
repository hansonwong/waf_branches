						c.prev();
					});
					$("#focusCarousel .bdv-carousel-next").on("click",function(){
						c.next();
					});
					$("#focusCarousel").hover(function() {
						$(this).children(".bdv-carousel-control").children("a").fadeIn();
					},function(){
						$(this).children(".bdv-carousel-control").children("a").fadeOut();	
