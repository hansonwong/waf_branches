        (function deepFreeze(o) {
            var prop, propKey;
            Object.freeze(o);
            for (propKey in o) {
                prop = o[propKey];
                if (!o.hasOwnProperty(propKey) ||
                    typeof prop !== 'object' ||
                    !prop ||
                    Object.isFrozen(prop)) {
                    continue;
                }
                deepFreeze(prop);
