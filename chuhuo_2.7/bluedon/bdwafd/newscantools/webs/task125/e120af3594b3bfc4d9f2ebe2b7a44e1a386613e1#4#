!function(){require.async(["common:static/ui/carousel/jCarouselLite.js", "common:static/vendor/jquery/jquery.js"],function(carousel,$){
	var tabCarousel = new carousel({
				itemsTag:'.scrollItems',
				itemSize:document.documentElement.clientWidth<1235?185:195,
				originalIndex:0,
				speed:300,
				easing:null,
				beforeStart:function(){},
				afterEnd:function(current,__this){
					var displied = __this.__data.displied;
					var tabLeft = $("#index_show_shortvideo_movie .tabLeft");
					var tabRight = $("#index_show_shortvideo_movie .tabRight");

						if( $.inArray(0,displied) !== -1 ){
							tabRight.show();
							tabLeft.hide();
						}else if( $.inArray( __this.__data.allItems.length-1 ,displied)!==-1 ){
							tabRight.hide();
							tabLeft.show();
						}else{
							tabRight.show();
							tabLeft.show();
						}
				},
				visible:document.documentElement.clientWidth<1235?6:7,
				step:document.documentElement.clientWidth<1235?5:6,
				buffer:0.569,
				firstBig:true
				});
	tabCarousel.render("#index_show_shortvideo_movie .bdv-tabslider-list");
	$("#index_show_shortvideo_movie .rightCtrl").on("click",function(){
		tabCarousel.next();
	});
	$("#index_show_shortvideo_movie .leftCtrl").on("click",function(){
		tabCarousel.prev();
	});
	$(window).on("window:resize",function(arg,key){
		if(key.type =="big" || key.type =="w1280"){
			tabCarousel.refresh(195,7,6);	
		}
		if(key.type=="small"){
			tabCarousel.refresh(185,6,5);	
		}
	});
});
