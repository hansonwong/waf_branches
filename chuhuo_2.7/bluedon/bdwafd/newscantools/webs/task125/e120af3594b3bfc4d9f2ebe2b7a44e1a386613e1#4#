	function createRcommendList(data){
		if(data && data.videoshow && data.videoshow.videos && data.videoshow.videos.length>0){
		$("#popu").show();
		data.videoshow.videos.length = 3;	
		document.getElementById("popu").className = "top-popu";
		$("#popu").html(temp({
				data:data.videoshow.videos,
				info:{
					"name":data.videoshow.typeName,
					"href":data.videoshow.typeHref,
					"bg":data.videoshow.background,
					"static":data.videoshow.static,
					"active":data.videoshow.active
					}
				})).addClass(data.videoshow.active);	
		}
	};

      function getDataSuccess(data,ele){
          if(!data.videoshow  || data.error==2){
              return false;
         }           
          ele.data("videos",data);
          data.videoshow["typeName"] = ele.attr("data-name");
          data.videoshow["active"] = ele.attr("data-active");
          data.videoshow["static"] = ele.attr("data-static");
          data.videoshow["typeHref"] = ele.find("a").attr("href");
          data.videoshow["background"] = ele.find("a").css("background-color");
          createRcommendList(ele.data("videos"));
      }   
      
      // 电影频道
	        window.tl_action    = function(data){getDataSuccess(data,$(".recmd-item").eq(0));window.tl_action   =function(){};}
      window.tl_comic     = function(data){getDataSuccess(data,$(".recmd-item").eq(1));window.tl_comic    =function(){};}
      window.tl_terror    = function(data){getDataSuccess(data,$(".recmd-item").eq(2));window.tl_terror   =function(){};}
      window.tl_fiction   = function(data){getDataSuccess(data,$(".recmd-item").eq(3));window.tl_fiction  =function(){};}
      window.tl_hongkong  = function(data){getDataSuccess(data,$(".recmd-item").eq(4));window.tl_hongkong =function(){};}
      window.tl_america   = function(data){getDataSuccess(data,$(".recmd-item").eq(5));window.tl_america  =function(){};}
	  

	  
	  





	$(".recmd-item").on("mouseover",function(){
		
		var __this = $(this);
		if(__this.attr("data-get") == "null" ){
			createRcommendList(__this.data("videos"))
