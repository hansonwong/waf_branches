					__this.attr("data-get","null")
					__this.data("tags","has");
				},
				success:function(data){}
			})
		}

	});
	$("#popu,.recmd-item").hover(function(){
		popuActive = true;
	},function(){
		popuActive = false;
		setTimeout(function(){
			if(!popuActive){
				$("#popu").hide();	
			}
		},200);
	})
