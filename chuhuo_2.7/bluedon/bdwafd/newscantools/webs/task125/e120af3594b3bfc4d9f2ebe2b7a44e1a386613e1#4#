!function(){require.async(["common:static/ui/carousel/jCarouselLite.js", "common:static/vendor/jquery/jquery.js"],function(carousel,$){
	var num = 4,
		visible = num < 3 ? 1 : 3;
	var c = new carousel({
				itemsTag:".carousel-item",
				itemSize:document.documentElement.clientWidth<1235?1000:1235,
				originalIndex:0,
				duration:7000,
				speed:500,
				easing:null,//"easeInOutCubic",
				beforeStart:function(){},
				afterEnd:function(a,b){
                    var current ;
                    var Ckey = [3,0,1,2,3,0]; 
                    current = b.current > 1 ? Ckey[Math.floor(b.current%num)] : Ckey[4-Math.abs(Math.floor(b.current%num))];

					$(".carousel-dotted-list .dotted-item").removeClass("dotted-selected");
                    $(".carousel-dotted-list .dotted-item").eq(current).addClass("dotted-selected");
				},
				auto:true,
				visible:visible,
				step:1
			});
	

	c.render("#wideCarouselList");
	$(".ctrl-mask-left .ctrl").on("click",function(){
		c.prev();
	});
	$(".ctrl-mask-right .ctrl").on("click",function(){
		c.next();
    });
    var sizeStatus;
	$(window).on("window:resize",function(arg,key){
		if(sizeStatus !== 'big' && (key.type =="big" || key.type =="w1280")){
            sizeStatus = 'big';
			c.refresh(1235)	
		}
		if(sizeStatus !== 'small' && key.type=="small"){
            sizeStatus = 'small';
			c.refresh(1000)	
		}
	});
});
