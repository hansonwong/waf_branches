           var index = 0 ;
           var url = location.href.substr(location.href.indexOf("?")+1).split("&");
           var key ;
           for(var i = 0 ; i < url.length ; i++){
               var tmp = url[i].split("=");
               if(tmp&&tmp.length>0&&tmp[0]=="lb"){
                   key = tmp[1];
               }
               }
           for(var p in imgList){
               if(imgList[p].url == key){
                   index = p ;
               }
           }
